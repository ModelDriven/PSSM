               <generalization xmi:type="uml:Generalization" xmi:id="_fiNSkFliEeaMpZSzvy8A5Q"
                               xmi:uuid="20f77277-27f7-4cbf-8ba3-80fc4119a3ec">
                  <general href="http://www.omg.org/spec/PSSM/20161101/fUML_Semantics.xmi#Semantics-CommonBehaviors-Communications-ObjectActivation"/>
