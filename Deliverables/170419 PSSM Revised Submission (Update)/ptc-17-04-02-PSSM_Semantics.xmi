                  <ownedParameter xmi:type="uml:Parameter" xmi:id="__3J50aWDEea42broHGSgMg"
                                  xmi:uuid="a8d9f699-ab35-47ff-bd5a-e783d6aa9f26"
                                  visibility="public"
                                  direction="return"
                                  effect="create">
                     <type href="http://www.omg.org/spec/UML/20161101/PrimitiveTypes.xmi#Boolean"/>
