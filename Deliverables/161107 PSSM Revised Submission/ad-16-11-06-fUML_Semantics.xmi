                              xmi:id="Semantics-Activities-CompleteStructuredActivities-StructuredActivityNodeActivation-isSourceFor"
                              xmi:uuid="27e46d50-7e40-4834-956a-c39d1b669d44"
                              name="isSourceFor"
                              specification="Semantics-Activities-CompleteStructuredActivities-StructuredActivityNodeActivation-isSourceFor.1">
                  <body>// Returns true if this node is either the source for the given
// edgeInstance itself or if it contains the source in its
// activation group.

boolean isSource = super.isSourceFor(edgeInstance);
if (!isSource) {
	isSource = this.activationGroup.hasSourceFor(edgeInstance);
}
return isSource;
</body>
