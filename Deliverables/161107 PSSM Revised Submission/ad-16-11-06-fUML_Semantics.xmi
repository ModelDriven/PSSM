               <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="Semantics-Classes-Kernel-Object-destroy"
                              xmi:uuid="ee11fc45-2abd-43cd-87a5-ca494502b899"
                              name="destroy"
                              specification="Semantics-Classes-Kernel-Object-destroy.1">
                  <body>// Stop the object activation (if any), clear all types and destroy the
// object as an extensional value.

Debug.println("[destroy] object = " + this.identifier);

if (this.objectActivation != null) {
	this.objectActivation.stop();
	this.objectActivation = null;
}

this.types.clear();
super.destroy();
</body>
                  <language>Java</language>
               </ownedBehavior>
