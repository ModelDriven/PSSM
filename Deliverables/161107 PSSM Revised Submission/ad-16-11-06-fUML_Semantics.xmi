                             xmi:uuid="ad527446-12e9-4212-a871-ecb6890347ee"
                             body="import fUML.Syntax.*;&#xD;&#xA;import fUML.Syntax.Classes.Kernel.*;&#xD;&#xA;import fUML.Syntax.CommonBehaviors.BasicBehaviors.*;&#xD;&#xA;import fUML.Syntax.CommonBehaviors.Communications.*;&#xD;&#xA;import fUML.Syntax.Activities.IntermediateActivities.*;&#xD;&#xA;import fUML.Syntax.Actions.BasicActions.*;&#xD;&#xA;&#xD;&#xA;import fUML.Semantics.*;&#xD;&#xA;import fUML.Semantics.Classes.Kernel.*;&#xD;&#xA;import fUML.Semantics.CommonBehaviors.BasicBehaviors.*;&#xD;&#xA;import fUML.Semantics.Actions.BasicActions.*;&#xD;&#xA;import fUML.Semantics.Loci.*;&#xD;&#xA;"/>
               <generalization xmi:type="uml:Generalization"
                               xmi:id="Semantics-Activities-IntermediateActivities-ObjectNodeActivation-_generalization.0"
                               xmi:uuid="cb8bcf7e-aa3b-4c0e-91fb-3962c47a4dea"
                               general="Semantics-Activities-IntermediateActivities-ActivityNodeActivation"/>
               <ownedBehavior xmi:type="uml:OpaqueBehavior"
                              xmi:id="Semantics-Activities-IntermediateActivities-ObjectNodeActivation-run"
                              xmi:uuid="7c28f2a9-f889-4ea3-a2c8-67b52106ec8b"
                              name="run"
                              specification="Semantics-Activities-IntermediateActivities-ObjectNodeActivation-run.1">
                  <body>// Initialize the offered token count to zero.

super.run();
this.offeredTokenCount = 0;
</body>
                  <language>Java</language>
               </ownedBehavior>
               <ownedBehavior xmi:type="uml:OpaqueBehavior"
                              xmi:id="Semantics-Activities-IntermediateActivities-ObjectNodeActivation-sendOffers"
                              xmi:uuid="50afb3b9-ccd9-4c2f-987c-c9018d30cb0d"
                              name="sendOffers"
                              specification="Semantics-Activities-IntermediateActivities-ObjectNodeActivation-sendOffers.1">
                  <body>// If the set of tokens to be sent is empty, then offer a null token
// instead.
// Otherwise, offer the given tokens as usual.

if (tokens.size() == 0) {
	ObjectToken token = new ObjectToken();
	token.holder = this;
	tokens.addValue(token);
}

super.sendOffers(tokens);
</body>
                  <language>Java</language>
                  <ownedParameter xmi:type="uml:Parameter"
