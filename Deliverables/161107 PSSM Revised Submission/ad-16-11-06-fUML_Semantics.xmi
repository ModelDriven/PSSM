               <ownedBehavior xmi:type="uml:OpaqueBehavior"
                              xmi:id="Semantics-Actions-BasicActions-ActionActivation-fire"
                              xmi:uuid="a9ca32db-c8ab-4f0a-9286-529e1d6667e1"
                              name="fire"
                              specification="Semantics-Actions-BasicActions-ActionActivation-fire.1">
                  <body>// Do the main action behavior then concurrently fire all output pin
// activations
// and offer a single control token. Then activate the action again,
// if it is still ready to fire and has at least one token actually
// being
// offered to it.

do {

	Debug.println("[fire] Action " + this.node.name + "...");
	Debug.println("[event] Fire activity="
			+ this.getActivityExecution().getBehavior().name
			+ " action=" + this.node.name);

	this.doAction();
	incomingTokens = this.completeAction();

} while (incomingTokens.size() &gt; 0);
</body>
