                              xmi:id="Semantics-CommonBehaviors-Communications-ClassifierBehaviorInvocationEventAccepter-match"
                              xmi:uuid="6701fd57-d784-46e8-87a1-b801d61b26ef"
                              name="match"
                              specification="Semantics-CommonBehaviors-Communications-ClassifierBehaviorInvocationEventAccepter-match.1">
                  <body>// Return true if the given event occurrence is an invocation event
// occurrence for the execution of this classifier behavior invocation
// event accepter.

boolean matches = false;
if (eventOccurrence instanceof InvocationEventOccurrence) {
	matches = ((InvocationEventOccurrence)eventOccurrence).execution == this.execution;
}
return matches;
</body>
