               <ownedBehavior xmi:type="uml:OpaqueBehavior"
                              xmi:id="Semantics-Activities-IntermediateActivities-ObjectNodeActivation-terminate"
                              xmi:uuid="1796c046-cbb3-45ce-9863-f6a6c45bf79b"
                              name="terminate"
                              specification="Semantics-Activities-IntermediateActivities-ObjectNodeActivation-terminate.1">
                  <body>// Remove any offered tokens and terminate.

this.clearTokens();
super.terminate();
</body>
                  <language>Java</language>
               </ownedBehavior>
               <ownedBehavior xmi:type="uml:OpaqueBehavior"
                              xmi:id="Semantics-Activities-IntermediateActivities-ObjectNodeActivation-addToken"
                              xmi:uuid="68f266b7-69ac-43b6-b15e-1b9af34c1655"
                              name="addToken"
                              specification="Semantics-Activities-IntermediateActivities-ObjectNodeActivation-addToken.1">
                  <body>// Transfer the given token to be held by this node only if it is a
// non-null object token.
// If it is a control token or a null token, consume it without holding
// it.

if (token.getValue() == null) {
	token.withdraw();
} else {
	super.addToken(token);
}
</body>
                  <language>Java</language>
                  <ownedParameter xmi:type="uml:Parameter"
                                  xmi:id="Semantics-Activities-IntermediateActivities-ObjectNodeActivation-addToken-token"
                                  xmi:uuid="885d156b-5b1b-40ce-a20d-f872849478b7"
                                  name="token"
                                  visibility="public"
                                  effect="create"
                                  type="Semantics-Activities-IntermediateActivities-Token"/>
               </ownedBehavior>
               <ownedBehavior xmi:type="uml:OpaqueBehavior"
                              xmi:id="Semantics-Activities-IntermediateActivities-ObjectNodeActivation-removeToken"
                              xmi:uuid="f8dffa86-1830-4bfe-a2ed-dfd39d37f185"
                              name="removeToken"
                              specification="Semantics-Activities-IntermediateActivities-ObjectNodeActivation-removeToken.1">
                  <body>// Remove the given token, if it is held by this node activation.

int i = super.removeToken(token);
if (i &gt; 0 &amp; i &lt;= this.offeredTokenCount) {
	this.offeredTokenCount = this.offeredTokenCount - 1;
}

return i;
</body>
                  <language>Java</language>
