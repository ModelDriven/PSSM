               <ownedBehavior xmi:type="uml:OpaqueBehavior"
                              xmi:id="Semantics-Classes-Kernel-LiteralBooleanEvaluation-evaluate"
                              xmi:uuid="62fa2fa0-b02d-40f9-af52-77ee2b069ac1"
                              name="evaluate"
                              specification="Semantics-Classes-Kernel-LiteralBooleanEvaluation-evaluate.1">
                  <body>// Evaluate a literal boolean, producing a boolean value.

LiteralBoolean literal = (LiteralBoolean) specification;
BooleanValue booleanValue = new BooleanValue();
booleanValue.type = this.getType("Boolean");
booleanValue.value = literal.value;

return booleanValue;

</body>
