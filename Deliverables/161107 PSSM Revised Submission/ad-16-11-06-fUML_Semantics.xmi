                              name="completeAction"
                              specification="Semantics-Activities-CompleteStructuredActivities-ConditionalNodeActivation-completeAction.1">
                  <body>// Only complete the conditional node if it is not suspended.

if (!this.isSuspended()) {
	completeBody();
}
return super.completeAction();
