               </ownedComment>
               <ownedComment xmi:type="uml:Comment" xmi:id="Semantics-Classes-Kernel-Link-_ownedComment.1"
                             xmi:uuid="97acde2f-8555-4d04-954d-1e63d96ff17e"
                             body="import fUML.Syntax.*;&#xD;&#xA;import fUML.Syntax.Classes.Kernel.*;&#xD;&#xA;&#xD;&#xA;import fUML.Semantics.*;&#xD;&#xA;import fUML.Semantics.Loci.*;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;"/>
               <generalization xmi:type="uml:Generalization"
                               xmi:id="Semantics-Classes-Kernel-Link-_generalization.0"
                               xmi:uuid="3951d232-089b-48f1-a467-ab2be1f2a473"
