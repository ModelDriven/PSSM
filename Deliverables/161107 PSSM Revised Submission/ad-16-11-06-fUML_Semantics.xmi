               <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="Semantics-Loci-LociL1-Locus-conforms"
                              xmi:uuid="f9ba9196-c8e0-4ebd-835e-1392f19c25cb"
                              name="conforms"
                              specification="Semantics-Loci-LociL1-Locus-conforms.1">
                  <body>// Test if a type conforms to a given classifier, that is, the type is
// equal to or a descendant of the classifier.

boolean doesConform = false;

if (type == classifier) {
	doesConform = true;
} else {
	int i = 1;
	while (!doesConform &amp; i &lt;= type.general.size()) {
		doesConform = this.conforms(type.general.getValue(i - 1),
				classifier);
		i = i + 1;
	}
}

return doesConform;

</body>
