               <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="Semantics-Loci-LociL1-Locus-instantiate"
                              xmi:uuid="c9a06b10-881d-4d39-8aa0-6d9a064a9a0d"
                              name="instantiate"
                              specification="Semantics-Loci-LociL1-Locus-instantiate.1">
                  <body>// Instantiate the given class at this locus.

Object_ object = null;

if (type instanceof Behavior) {
	object = this.factory.createExecution((Behavior) type, null);
} else {
	object = new Object_();

	object.types.addValue(type);
	object.createFeatureValues();
	this.add(object);
}

return object;
</body>
