               </ownedBehavior>
               <ownedBehavior xmi:type="uml:OpaqueBehavior"
                              xmi:id="Semantics-Activities-ExtraStructuredActivities-ExpansionActivationGroup-suspend"
                              xmi:uuid="24556fe9-afe9-4ec9-94c8-370b75ebcf8a"
                              name="suspend"
                              specification="Semantics-Activities-ExtraStructuredActivities-ExpansionActivationGroup-suspend.1">
                  <body>// Suspend the given activation in this activation group. If this is
// the only suspended activation, then suspend the associated region
// activation.

if (!this.isSuspended()) {
	this.regionActivation.suspend();
}
super.suspend(activation);
</body>
                  <language>Java</language>
               </ownedBehavior>
               <ownedBehavior xmi:type="uml:OpaqueBehavior"
                              xmi:id="Semantics-Activities-ExtraStructuredActivities-ExpansionActivationGroup-resume"
                              xmi:uuid="4376ec54-4c4a-4183-b934-b32b7bb2e1a8"
                              name="resume"
                              specification="Semantics-Activities-ExtraStructuredActivities-ExpansionActivationGroup-resume.1">
                  <body>// Resume the given activation in this activation group. If this is the
// last suspended activation, then resume the associated region
// activation.

super.resume(activation);
if (!this.isSuspended()) {
	this.regionActivation.resume(this);
}
</body>
                  <language>Java</language>
