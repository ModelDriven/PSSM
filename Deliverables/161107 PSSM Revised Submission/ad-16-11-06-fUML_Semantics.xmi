               <ownedBehavior xmi:type="uml:OpaqueBehavior"
                              xmi:id="Semantics-Activities-CompleteStructuredActivities-StructuredActivityNodeActivation-completeAction"
                              xmi:uuid="555caa46-1555-4052-88c4-9213c62d00e2"
                              name="completeAction"
                              specification="Semantics-Activities-CompleteStructuredActivities-StructuredActivityNodeActivation-completeAction.1">
                  <body>// Only actually complete this structured activity node if it is not
// suspended.

TokenList incomingTokens = new TokenList();
if (!this.isSuspended()) {
	incomingTokens = super.completeAction();
}
return incomingTokens;
</body>
                  <language>Java</language>
               </ownedBehavior>
               <ownedBehavior xmi:type="uml:OpaqueBehavior"
                              xmi:id="Semantics-Activities-CompleteStructuredActivities-StructuredActivityNodeActivation-resume"
                              xmi:uuid="e44a0db6-3540-4487-a404-7c57abf32adb"
                              name="resume"
                              specification="Semantics-Activities-CompleteStructuredActivities-StructuredActivityNodeActivation-resume.1">
                  <body>// When this structured activity node is resumed after being suspended,
// then complete its prior firing and, if there are more incoming
// tokens, fire it again. If, after that, the node is not suspended,
// then finish its resumption.

TokenList incomingTokens = super.completeAction();
if (incomingTokens.size() &gt; 0) {
	this.fire(incomingTokens);
}
if (!this.isSuspended()) {
	super.resume();
}
</body>
