                  <ownedParameter xmi:type="uml:Parameter"
                                  xmi:id="Semantics-Activities-CompleteStructuredActivities-ConditionalNodeActivation-selectBody-clause"
                                  xmi:uuid="894eac12-ac61-4fef-8f1c-c8ebd48b0402"
                                  name="clause"
