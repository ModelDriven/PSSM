               <ownedBehavior xmi:type="uml:OpaqueBehavior"
                              xmi:id="Semantics-Classes-Kernel-RedefinitionBasedDispatchStrategy-operationsMatch"
                              xmi:uuid="01eed5e2-82ab-48fd-8449-e2d0118119fc"
                              name="operationsMatch"
                              specification="Semantics-Classes-Kernel-RedefinitionBasedDispatchStrategy-operationsMatch.1">
                  <body>// Check if the owned operation is equal to or a redefinition (directly
// or indirectly) of the base operation.

boolean matches = false;
if (ownedOperation == baseOperation) {
	matches = true;
} else {
	int i = 1;
	while (!matches &amp; i &lt;= ownedOperation.redefinedOperation.size()) {
		matches = this.operationsMatch(
				ownedOperation.redefinedOperation.getValue(i - 1),
				baseOperation);
		i = i + 1;
	}
}

return matches;
</body>
