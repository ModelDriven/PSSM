                              xmi:uuid="00b9c735-2082-4802-82f8-330bf1675d02"
                              name="unregister"
                              specification="Semantics-CommonBehaviors-Communications-ObjectActivation-unregister.1">
                  <body>// Remove the given event accepter for the list of waiting event
// accepters.

Debug.println("[unregister] object = " + this.object);
Debug.println("[unregister] accepter = " + accepter);

boolean notFound = true;
int i = 1;
while (notFound &amp; i &lt;= this.waitingEventAccepters.size()) {
	if (this.waitingEventAccepters.getValue(i - 1) == accepter) {
		this.waitingEventAccepters.remove(i - 1);
		notFound = false;
	}
	i = i + 1;
}

</body>
