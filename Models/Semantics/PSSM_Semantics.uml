            <ownedParameter xmi:id="__xNC06WDEea42broHGSgMg" direction="return">
              <name xsi:nil="true"/>
              <type xmi:type="uml:Class" href="fUML_Semantics.uml#Semantics-CommonBehaviors-BasicBehaviors-ParameterValue"/>
              <lowerValue xmi:type="uml:LiteralInteger" xmi:id="__xNC1KWDEea42broHGSgMg" value="1"/>
              <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="__xNC1aWDEea42broHGSgMg" value="1"/>
            </ownedParameter>
            <language>Java</language>
            <body>// Return the parameter value corresponding to the given parameter.
// Null is returned if no parameter value associated with the parameter
// is found.
int i = 0;
ParameterValue parameterValue = null;
while(parameterValue == null &amp;&amp; i &lt; this.parameterValues.size()){
	if(this.parameterValues.get(i).parameter == parameter){
		parameterValue = this.parameterValues.get(i);
	}
	i++;
}
return parameterValue;
</body>
