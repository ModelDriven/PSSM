            <ownedParameter xmi:id="__xq85KWDEea42broHGSgMg" name="object">
              <type xmi:type="uml:Class" href="fUML_Semantics.uml#Semantics-Classes-Kernel-Object"/>
              <lowerValue xmi:type="uml:LiteralInteger" xmi:id="__xq85aWDEea42broHGSgMg" value="1"/>
              <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="__xq85qWDEea42broHGSgMg" value="1"/>
            </ownedParameter>
            <ownedParameter xmi:id="__xq856WDEea42broHGSgMg" name="operation">
              <type xmi:type="uml:Class" href="../Syntax/UML.uml#Operation"/>
              <lowerValue xmi:type="uml:LiteralInteger" xmi:id="__xq86KWDEea42broHGSgMg" value="1"/>
              <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="__xq86aWDEea42broHGSgMg" value="1"/>
            </ownedParameter>
            <language>Java</language>
            <body>// Behaves as in fUML and PSCS except that if no method is declared
// for the operation then null is returned.
Behavior method = null;
int i = 1;
while (method == null &amp; i &lt;= object.types.size()) {
	Class type = object.types.get(i - 1);
	List&lt;NamedElement> members = type.getMembers();
	int j = 1;
	while (method == null &amp; j &lt;= members.size()) {
		NamedElement member = members.get(j - 1);
		if (member instanceof Operation) {
			Operation memberOperation = (Operation) member;
			if (this.operationsMatch(memberOperation, operation)) {
				if(memberOperation.getMethods().size() > 0){
					method = memberOperation.getMethods().get(0);
				}
			}
		}
		j = j + 1;
	}
	i = i + 1;
}
return method;
</body>
