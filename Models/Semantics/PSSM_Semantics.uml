            <ownedParameter xmi:id="__0eZQaWDEea42broHGSgMg" name="node">
              <type xmi:type="uml:Class" href="../Syntax/UML.uml#NamedElement"/>
              <lowerValue xmi:type="uml:LiteralInteger" xmi:id="__0eZQqWDEea42broHGSgMg" value="1"/>
              <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="__0eZQ6WDEea42broHGSgMg" value="1"/>
