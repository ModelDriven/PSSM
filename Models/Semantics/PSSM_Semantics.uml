              <lowerValue xmi:type="uml:LiteralInteger" xmi:id="_0mbhUtQuEeWMJphss3BZSg" value="1"/>
              <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="_0mbhU9QuEeWMJphss3BZSg" value="1"/>
