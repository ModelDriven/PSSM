          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="__00-kKWDEea42broHGSgMg" name="isVisitorFor" specification="_7NHs8Fi-EeaMpZSzvy8A5Q">
            <ownedParameter xmi:id="__00-kaWDEea42broHGSgMg" name="node">
              <type xmi:type="uml:Class" href="../Syntax/UML.uml#NamedElement"/>
              <lowerValue xmi:type="uml:LiteralInteger" xmi:id="__00-kqWDEea42broHGSgMg" value="1"/>
              <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="__00-k6WDEea42broHGSgMg" value="1"/>
            </ownedParameter>
            <ownedParameter xmi:id="__00-lKWDEea42broHGSgMg" direction="return">
              <name xsi:nil="true"/>
