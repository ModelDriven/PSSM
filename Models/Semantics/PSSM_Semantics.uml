          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="__xq84KWDEea42broHGSgMg" name="getMethod" specification="_-jy2cFgREeaqSKm88dX50g">
            <ownedParameter xmi:id="__xq84aWDEea42broHGSgMg" name="ReturnResult" direction="return">
