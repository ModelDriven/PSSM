          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_tr3l0BE6EeKT3tsL4v_Daw" name="new_" specification="_JQJXABE3EeKT3tsL4v_Daw">
            <ownedParameter xmi:id="_wwhCkBE6EeKT3tsL4v_Daw" name="result" direction="return">
