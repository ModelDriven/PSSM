          <ownedEnd xmi:id="_wUwzIt2JEeGnO-TF_1T_iQ" name="signalInstanceWithPort" type="_wUwzEN2JEeGnO-TF_1T_iQ" association="_wUwzId2JEeGnO-TF_1T_iQ">
