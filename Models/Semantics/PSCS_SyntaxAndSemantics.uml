          <ownedOperation xmi:id="_itG3kMEVEeKkJbQgitQ3JQ" name="executeExpressionBehavior" method="_9xH3gMEVEeKkJbQgitQ3JQ">
            <ownedParameter xmi:id="_oTWuYMEVEeKkJbQgitQ3JQ" name="result" direction="return">
