              <lowerValue xmi:type="uml:LiteralInteger" xmi:id="_rcC9oMEVEeKkJbQgitQ3JQ"/>
              <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="_rcMuoMEVEeKkJbQgitQ3JQ" value="*"/>
            </ownedParameter>
